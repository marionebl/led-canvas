{
  "name": "led-canvas",
  "version": "0.0.1",
  "description": "Lightweight led board implemented with canvas",
  "main": "lib/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "prepublish": "npm run build",
    "postpublish": "npm run reset",
    "prepare": "mkdir -p ./lib && mkdir -p ./dist",
    "reset": "rm -r ./lib && rm -r ./dist",
    "sixtofive": "6to5 ./src --out-dir ./lib",
    "browserify": "browserify ./lib/index.js -o ./dist/index.js",
    "build": "npm run prepare && npm run sixtofive",
    "dist": "npm run prepare && npm run browserify"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/marionebl/led-canvas.git"
  },
  "keywords": [
    "led",
    "canvas",
    "led-board"
  ],
  "author": "Mario Nebl <hello@mario-nebl.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marionebl/led-canvas/issues"
  },
  "homepage": "https://github.com/marionebl/led-canvas",
  "dependencies": {
    "led-canvas-matrix": "0.0.1",
    "object-assign": "^1.0.0"
  },
  "devDependencies": {
    "6to5": "^1.9.10",
    "browserify": "^6.3.2"
  }
}
